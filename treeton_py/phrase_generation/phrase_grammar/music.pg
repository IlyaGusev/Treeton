# любимые песни Алисы

like_passive_clause:
  root: common.finite_clause
  action:
    root:
      extends: common.abstract_action
      gramm: ['3']
      lex: [нравиться]
    dative_object:
      lex: [ты]  

like_active_clause:
  root: common.finite_clause
  action:
    root:
      extends: common.abstract_action
      gramm: ['2','act','sing']
      lex: [любить,обожать]
    subject:
      lex: [ты]


favorite_alice_music:
  onto: 
    id: alice
  content:
    - root:
        extends: common.abstract_object
        content:
          - root: 
              - abstract_music_keyword
              - playlist_keyword
            attr_uniq:
              gramm: [adj]
              lex: [любимый]
          - playlist_keyword
      attr:
        reference: id
        lex: [свой, твой]
      relative:
        reference: id
        content:
          - root:
              gramm: [nom]
              content: common.relative_word
            relative_clause_agree: like_passive_clause
          - root:
              gramm: [acc]
              content: common.relative_word
            relative_clause: like_active_clause
    - root: common.relative_pro
      relative_clause_agree:
        reference: id
        content: like_passive_clause
      relative_clause:
        reference: id
        content: like_active_clause

