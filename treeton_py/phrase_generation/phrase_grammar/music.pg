more_less_preambula:
  extends: [common.abstract_action]
  root:
    extends: [common.abstract_object]
  any_uniq_indecl:
    lex: 
      - сделай
      - пусть будет
      - пусть играет
      - теперь

music_fraction:
  root:
    extends: common.abstract_object
    lex: [доля,процент]
  genetive_uniq:
    - abstract_music
    - artist_mention

switch_on_more_music:
  onto:
    type: more
    target: {}
  content:
    - root: common.infinitive_clause
      infinitive:
        - root: all_music_verbs
          object: more_music
        - root:
            extends: common.abstract_action
            tag: modifier
            lex: [увеличить,поднять]
          object: music_fraction
    - root: common.imperative_clause
      imperative:
        - root:
            extends: common.abstract_action
            tag: modifier
            lex: [увеличить,увеличивать,поднять,поднимать]
          object: music_fraction
        - root: 
            gramm: [sing]
            content: all_launch_verbs
          object: more_music
    - root: more_less_preambula
      object: more_music

switch_on_less_music:
  onto:
    type: less
    target: {}
  content:
    - root: common.infinitive_clause
      infinitive:
        - root: all_music_verbs
          object: less_music
        - root:
            extends: common.abstract_action
            tag: modifier
            lex: [уменьшить,снизить,понизить]
          object: music_fraction
    - root: common.imperative_clause
      imperative:
        - root:
            extends: common.abstract_action
            tag: modifier
            lex: [уменьшить,уменьшать,снизить,снижать,понизить,понижать]
          object: music_fraction
        - root: 
            gramm: [sing]
            content: all_launch_verbs
          object: less_music
    - root: more_less_preambula
      object: less_music


