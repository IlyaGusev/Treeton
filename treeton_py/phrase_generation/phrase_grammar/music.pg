# music verbs

music_action:
  extends: common.abstract_action

launch_imperf:
  extends: music_action
  lex:
    - запускать
    - играть
    - проигрывать
    - ставить
    - врубать
    - включать
    - воспроизводить

launch_perf:
  extends: music_action
  lex:
    - запустить
    - сыграть
    - проиграть
    - плей
    - play
    - поставить
    - врубить
    - включить
    - воспроизвести

listen_perf:
  extends: music_action
  lex:
    - услышать
    - прослушать
    - послушать

listen_imperf:
  extends: music_action
  lex:
    - слушать
    - прослушивать
    - слышать

all_autoplay_launch_verbs:
  tag: action_request
  content:
    - launch_perf
    - launch_imperf

all_autoplay_listen_verbs:
  tag: action_request
  content:
    - listen_perf
    - listen_imperf

all_autoplay_verbs:
  - all_autoplay_launch_verbs
  - all_autoplay_listen_verbs

non_autoplay_verbs:
  extends: music_action
  content:
    - root:
        extends: common.abstract_action
        lex: скачать
      conj_right: all_autoplay_listen_verbs
    - lex:
      - скачать
      - найти
      - открыть
      - дать
      - показать

all_launch_verbs:
  - all_autoplay_launch_verbs
  - non_autoplay_verbs

all_music_verbs:
  - all_autoplay_verbs
  - non_autoplay_verbs


# music objects

abstract_music_keyword:
  extends: common.abstract_object
  content:
    - gramm: [sing]
      lex:
        - музыка
        - музон
        - музяка
        - музло
        - песня
    - common.what_word
    - gramm: [plur]
      lex: [песня]


abstract_music:
  onto:
    type: collection
    proto:
      type: track
  content:
    root:
      reference: proto
      content:
        root:
          - abstract_music_keyword
          - genre
          - root:
              extends: common.abstract_object
              gramm: [plur]
              lex: [хит]
            genetive_oblig: genre
        purpose:
          - activity_purpose
          - mood_purpose
          - root:
              extends: common.abstract_named_object
              gramm: [sing]
              lex: [прослушивание,воспроизведение,проигрывание,игра,звучание]
            inside_oblig: activity_area
        attr:
          - activity_attr
          - epoch_attr
          - language_attr
          - mood_attr
          - genre_attr
        on_the:
          - language_language
        with:
          - language_words
        any_indecl: epoch_indecl
        inside_acc:
          gramm: [sing]
          content: activity_area
        inside:
          - root:
              extends: common.abstract_named_object
              gramm: [sing]
              lex: [жанр, стиль]
            name: 
              - genre
              - root: 
                  extends: common.abstract_object
                  content:
                    - gramm: [sing]
                      lex: [музыка,песня]
                    - gramm: [plur]
                      lex: [песня]
                attr_uniq: genre_attr
          - reference: album
            content: album_mention
          - reference: playlist
            content: playlist_mention
        indecl_genet: epoch_genet
        genetive:
          - genre_area
          - reference: artist
            content: artist_mention
        within:
          - genre_by_source
          - reference: album
            content: album_mention
          - reference: playlist
            content: playlist_mention
        for:
          onto:
            genre: forchildren
          root:
            reference: genre
            tag: genre
            lex: [малыш,ребенок,сын,дочь,малышка]

# языки

language_attr:
  onto:
    language: {}
  root:
    tag: language
    reference: language
    content:
      - onto: russian
        lex: [родной,отечественный,русский,русскоязычный,наш]  
      - onto: foreign
        lex: [иностранный,западный]
      - onto: foreign
        root:
          extends: common.abstract_entity
          lex: [родной,русский,славянский,русскоязычный,наш]
        proclitic:
          lex: [не]


language_words:
  onto:
    language: {}
  root:
    extends: common.abstract_object
    content:
      - lex: [текст]
      - gramm: [plur]
        lex: [слово]  
      - gramm: [sing,fem]
        lex: [лирика]
  attr_uniq: language_attr

language_language:
  onto:
    language: {}
  root:
    extends: common.abstract_object
    gramm: [sing]
    lex: [язык]  
  attr_uniq: language_attr


# эпохи

epoch_indecl:
  onto:
    epoch: {}
  root:
    tag: epoch
    reference: epoch
    content:
      - onto: the-greatest-hits
        lex: [на все времена]  
      - onto: nineties
        lex: [времен перестройки]

epoch_attr:
  onto:
    epoch: {}
  root:
    tag: epoch
    reference: epoch
    content:
      - onto: the-greatest-hits
        lex: [вечный, золотой]  
      - onto: nineties
        lex: [перестроечный]

epoch_genet:
  onto:
    epoch: {}
  root:
    extends: common.abstract_object
    content:
      - lex: [годов,годков]
      - {}
  any_uniq_indecl:
    tag: epoch
    reference: epoch
    content:
      - onto: fifties
        lex: [50-х,пятидесятых,'50']
      - onto: sixties
        lex: [60-х,шестидесятых,'60']
      - onto: seventies
        lex: [70-х,семидесятых,'70']
      - onto: eighties
        lex: [80-х,восьмидесятых,'80']
      - onto: nineties
        lex: [90-х,девяностых,'90']
      - onto: zeroes
        lex: [нулевых]      

