all_music:
  - abstract_music
  - album_mention
  - playlist_mention
  - artist_mention
  - track_mention

# на повторе/шафл

shuffle_phrase:
  tag: shuffle
  root:
    reference: shuffle
    lex: 
        - вперемешку
        - в произвольном порядке
        - шафл
        - рандомно
        - на рандоме
        - в рандомном порядке
        - в рандоме
        - в любом порядке
        - в смешанном порядке

shuffle_action:
  tag: shuffle
  root:
    reference: shuffle
    lex: 
      - перемешать
      - замешать
      - смешать

repeat_phrase:
  tag: repeat
  root:
    reference: repeat
    lex: 
      - на повторе
      - на повтор
      - на репите
      - циклом
      - в цикле
      - по кругу

repeat_action:
  tag: repeat
  root:
    reference: repeat
    lex: [закольцевать]

# основной интент включения музыки

fuzzy_phrase:
  lex:
    - бесплатно
    - 'без смс'
    - 'без регистрации'
    - mp3
    - мп3
    - 'мп 3'        
    - онлайн
    - 'Яндекс Музыка'
    - Яндекс.Музыка
    - 'на Яндекс Музыке'
    - 'на телефон'
    - пожалуйста
    - 'в хорошем качестве'
    - плиз
    - прошу
    - очень прошу
    - будьте добры
    - будь добра

let_it_play:
  extends: common.abstract_action
  root:
    extends: common.abstract_object
  any_uniq_indecl:
    lex: 
      - пусть играет
      - пусть звучит
      - пусть заиграет
      - пусть зазвучит

turn_music_on:
  onto:
    type: switch_on
    target: {}
  content:
    - root: common.infinitive_clause
      infinitive:
        onto:
          target: {}
        content:
          root: 
            - all_music_verbs
            - root: 
                - all_autoplay_listen_verbs
                - extends: common.abstract_action
                  lex: [найти, скачать]
              proclitic_oblig:
                lex: [хочу,'я хочу','мне надо']
            - let_it_play
          object:
            reference: target
            content: all_music
          circ_indecl:
            - shuffle_phrase
            - repeat_phrase
            - fuzzy_phrase
          conj_right:
            - shuffle_action
            - repeat_action
            - lex: [скачать]
    - root: common.imperative_clause
      imperative:
        onto:
          target: {}
        content:
          root: 
            gramm: [sing]
            content: all_launch_verbs
          object:
            reference: target
            content: 
              - all_music
              - special_music
          circ_indecl:
            - shuffle_phrase
            - repeat_phrase
            - fuzzy_phrase
          conj_right:
            - shuffle_action
            - repeat_action
            - lex: [скачать]
    - root: common.abstract_clause
      oblig_aux: all_music_verbs
      aux:
        - reference: target
          content: all_music
        - fuzzy_phrase
    - root: 
        extends: common.abstract_action


